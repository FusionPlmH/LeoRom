<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/sec_auto_power_on_off_title" android:key="auto_power_on_off_mode"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings.autopoweronoff">
    <com.samsung.android.settings.UnclickablePreference android:title="@string/sec_auto_power_on_off_description" android:selectable="false" android:key="sec_auto_power_on_off_description" />
    <com.samsung.android.settings.widget.SecInsetCategoryPreference android:key="sec_auto_power_on" />
    <SecSwitchPreference android:persistent="false" android:title="@string/sec_auto_power_on" android:key="auto_power_on_switch" />
    <SecPreference android:title="@string/sec_auto_power_on_off_time" android:key="auto_power_on_time" android:dependency="auto_power_on_switch" />
    <com.samsung.android.settings.autopoweronoff.AutoPowerOnOffDaysPreference android:title="@string/sec_auto_power_on_off_days" android:key="auto_power_on_days" android:dependency="auto_power_on_switch" />
    <com.samsung.android.settings.widget.SecInsetCategoryPreference android:key="auto_power_off" />
    <SecSwitchPreference android:persistent="false" android:title="@string/sec_auto_power_off" android:key="auto_power_off_switch" />
    <SecPreference android:title="@string/sec_auto_power_on_off_time" android:key="auto_power_off_time" android:dependency="auto_power_off_switch" />
    <com.samsung.android.settings.autopoweronoff.AutoPowerOnOffDaysPreference android:title="@string/sec_auto_power_on_off_days" android:key="auto_power_off_days" android:dependency="auto_power_off_switch" />
</PreferenceScreen>