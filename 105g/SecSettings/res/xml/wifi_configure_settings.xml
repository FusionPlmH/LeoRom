<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/wifi_menu_advanced_button" android:key="wifi_configure_settings_screen"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:settings="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory android:title="@string/wifi_intelligent_wifi_header" android:key="advanced_adaptive_wifi" android:order="1">
        <SecSwitchPreferenceScreen android:title="@string/wifi_switch_to_mobile_data" android:key="wifi_poor_network_detection" android:summary="@string/wifi_use_mobile_data" android:fragment="com.samsung.android.settings.wifi.SmartNetworkSwitchSettings" />
        <SwitchPreferenceCompat android:persistent="true" android:title="@string/wifi_autowifi_title" android:key="auto_wifi" android:summary="@string/wifi_autowifi_summary" />
        <SecSwitchPreferenceScreen android:title="@string/wifi_mobile_wips" android:key="MobileWIPS" android:summary="@string/wifi_mobile_wips_detail" android:fragment="com.samsung.android.settings.wifi.MobileWIPSsettings" />
        <SwitchPreferenceCompat android:persistent="true" android:title="@string/wifi_adps_title" android:key="wifi_adps" android:summary="@string/wifi_adps_summary" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/wifi_network_settings_category" android:key="advanced_network_settings" android:order="2">
        <Preference android:title="@string/secure_wifi" android:key="secure_wifi" android:summary="@string/secure_wifi_summary" />
        <SwitchPreferenceCompat android:persistent="true" android:title="@string/auto_connect_xfinity_wifi" android:key="auto_connect_xfinity_wifi" android:summary="@string/auto_connect_xfinity_wifi_summary" />
        <Preference android:persistent="false" android:title="@string/wifi_cricket_manager_title" android:key="cricket_manager" android:summary="@string/wifi_cricket_manager_summary" />
        <SecSwitchPreference android:persistent="true" android:title="@string/optimize_charter_wifi_networks" android:key="optimize_charter_wifi_networks" android:summary="@string/optimize_charter_wifi_networks_summary" />
        <SecSwitchPreference android:persistent="true" android:title="@string/wifi_att_auto_connect" android:key="att_auto_connect" android:summary="@string/wifi_att_auto_connect_summary" />
        <SwitchPreferenceCompat android:persistent="true" android:title="@string/wifi_notify_open_networks" android:key="notify_open_networks" android:summary="@string/wifi_notify_open_networks_summary" />
        <SwitchPreferenceCompat android:persistent="true" android:title="@string/wifi_notify_open_networks" android:key="notify_wifi_recommend" android:summary="@string/wifi_notify_open_networks_summary" />
        <SwitchPreferenceCompat android:persistent="true" android:title="@string/wlan_permission_available" android:key="wlan_permission_available" android:summary="@string/wlan_permission_available_message" />
        <SwitchPreferenceCompat android:persistent="true" android:visibility="gone" android:title="@string/notify_me_available_network" android:key="show_wifi_popup" android:summary="@string/notify_me_available_network_summary" />
        <ListPreference android:persistent="false" android:entries="@array/wifi_connect_entries" android:title="@string/wifi_connect_type_title" android:key="wifi_connection_type" android:entryValues="@array/wifi_connect_values" />
        <com.samsung.android.settings.wifi.CMCCWarningDialogPreference android:persistent="false" android:title="@string/cmcc_warning_dialog_control" android:key="wlan_notify_cmcc" android:dialogTitle="@string/cmcc_warning_dialog_control" android:dialogMessage="@string/cmcc_warning_dialog_control_summary" />
        <SwitchPreferenceCompat android:persistent="true" android:title="@string/wifi_scan_for_free_wlans_title" android:key="wifi_scan_for_free_wlans" android:summary="@string/wifi_scan_for_free_wlans_summary" />
        <SecSwitchPreference android:persistent="true" android:title="@string/tencent_wifi_detection_title" android:key="tencent_wifi_security_detection" android:summary="@string/tencent_wifi_detection_summary" />
        <Preference android:persistent="false" android:title="@string/wifi_manage_network_title" android:key="wifi_manage_network" android:summary="@string/wifi_manage_network_summary" />
    </PreferenceCategory>
    <Preference android:persistent="false" android:title="@string/wifi_control_history_title" android:key="wifi_control_history" android:summary="@string/wifi_control_history_summary" />
    <SwitchPreferenceCompat android:persistent="true" android:title="@string/wifi_hotspot20_enable" android:key="wifi_hs20_enable" android:summary="@string/wifi_hotspot20_summary" />
    <Preference android:persistent="false" android:title="@string/wifi_install_network_certificates" android:key="install_credentials" android:summary="@string/wifi_install_network_credentials_summary" />
    <Preference android:key="mac_address" />
</PreferenceScreen>